<framework>
    <execution>
        <clock sleep="true" unit="hz" value="100" />
        <mainThread />
    </execution>
    <modulesToEnable>
        <module>test_image_generator1</module>
        <module>test_image_generator2</module>
        <module>test_image_generator3</module>
        <module>sdl_image_renderer</module>
    </modulesToEnable>
    <service>
        <name>sdl_service</name>
    </service>
    <module>
        <name>test_image_generator1</name>
        <realName>test_image_generator</realName>
        <config>
            <type>1</type>
        </config>
    </module>
    <module>
        <name>test_image_generator2</name>
        <realName>test_image_generator</realName>
        <channelHint name="IMAGE" mapTo="IMAGE2" />
        <config>
            <type>2</type>
        </config>
    </module>
    <module>
        <name>test_image_generator3</name>
        <realName>test_image_generator</realName>
        <channelHint name="IMAGE" mapTo="IMAGE3" />
        <config>
            <type>3</type>
        </config>
    </module>
    <module>
        <name>sdl_image_renderer</name>
        <config>
            <title>My Window</title>
            <channels>IMAGE,IMAGE2,IMAGE3</channels>
            <key.down.79>
                <command>nav</command>
                <content>right</content>
            </key.down.79>
            <key.down.80>
                <command>nav</command>
                <content>left</content>
            </key.down.80>
            <key.down.81>
                <command>nav</command>
                <content>down</content>
            </key.down.81>
            <key.down.82>
                <command>nav</command>
                <content>up</content>
            </key.down.82>
            <key.down.44>
                <repeat>false</repeat>
                <command>nav</command>
                <content>space</content>
            </key.down.44>
        </config>
    </module>
</framework>
